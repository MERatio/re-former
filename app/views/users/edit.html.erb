<%= form_for @user do |f| %>
  <%= f.label(:username, 'Your user name here: ') %>
  <%= f.text_field(:username) %>
  <%= f.label(:email, 'Your email here: ') %>
  <%= f.email_field(:email) %>
  <%= f.label(:password, 'Your password here: ') %>
  <%= f.password_field(:password) %>
  <%= f.submit('Submit') %>
<% end %>
